    <validationRules>
        <fullName>Prevent_End_Before_Start</fullName>
        <active>true</active>
        <errorConditionFormula>IF ( OR( ISBLANK(SO_Model_Year_End__c ),ISBLANK(SO_Model_Year_Start__c ) ), FALSE,
VALUE( SO_Model_Year_End__c ) &lt;  VALUE( SO_Model_Year_Start__c ) )</errorConditionFormula>
        <errorDisplayField>SO_Model_Year_End__c</errorDisplayField>
        <errorMessage>The model year end cannot be before the model year start.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Year_End_is_Valid_Number</fullName>
        <active>true</active>
        <description>The field must be text due to Salesforce number formatting adding a &quot;,&quot; thus validation rule to ensure entered value is a number.</description>
        <errorConditionFormula>IF ( ISBLANK(SO_Model_Year_End__c ), FALSE, 
NOT (  ISNUMBER( SO_Model_Year_End__c ) ) || 
LEN ( SO_Model_Year_End__c ) &lt; 4    )</errorConditionFormula>
        <errorDisplayField>SO_Model_Year_End__c</errorDisplayField>
        <errorMessage>The model year end must be a number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Year_Start_is_Valid_Number</fullName>
        <active>true</active>
        <description>The field must be text due to Salesforce number formatting adding a &quot;,&quot; thus validation rule to ensure entered value is a number.</description>
        <errorConditionFormula>IF ( ISBLANK(SO_Model_Year_Start__c ), FALSE,
NOT ( ISNUMBER( SO_Model_Year_Start__c ) ) || 
LEN ( SO_Model_Year_Start__c ) &lt; 4  )</errorConditionFormula>
        <errorDisplayField>SO_Model_Year_Start__c</errorDisplayField>
        <errorMessage>The model year start must be a number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Creation_of_Series_for_SVO</fullName>
        <active>true</active>
        <description>Prevent creation of Series for Non-SVO Models</description>
        <errorConditionFormula>ISNEW() &amp;&amp; ($Permission.SO_Bespoke || $Permission.SO_Private_Office ) &amp;&amp;  SO_Model__r.SO_Is_Available_SVO__c == FALSE</errorConditionFormula>
        <errorMessage>You cannot create Series under the selected Model</errorMessage>
    </validationRules>
