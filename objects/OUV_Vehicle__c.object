NOT( ISCHANGED( OUV_ByPassValidation__c )) &amp;&amp;
ISCHANGED( OUV_VehicleMasterStatus__c ) &amp;&amp; ISPICKVAL(PRIORVALUE(OUV_VehicleMasterStatus__c ),&apos;Pre-Live&apos;) ) 
