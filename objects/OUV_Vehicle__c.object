    <validationRules>
        <fullName>Check_Full_VIN</fullName>
        <active>true</active>
        <description>Before turning Vehicle Live, the system should validate whether FULL VIN is populated in OUV Vehicle</description>
        <errorConditionFormula>AND(NOT( ISCHANGED( OUV_ByPassValidation__c ) ),ISCHANGED(OUV_VehicleMasterStatus__c),(LEN (OUV_FullVIN__c) = 0), (TEXT(OUV_VehicleMasterStatus__c) = &apos;Live&apos;))</errorConditionFormula>
        <errorMessage>Before turning Vehicle Status to Live, the Full VIN field must be populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OUV_Vehicle_PreSold</fullName>
        <active>true</active>
        <description>Expected Hand in Date cannot be changed when Vehicle Pre sold is checked</description>
        <errorConditionFormula>IF( AND(NOT( ISCHANGED( OUV_ByPassValidation__c ) ),OUV_Presold__c   = True),IF( ISCHANGED(  OUV_NewRequestedHandedInDate__c ) , True, false) , false)</errorConditionFormula>
        <errorDisplayField>OUV_NewRequestedHandedInDate__c</errorDisplayField>
        <errorMessage>This field cannot be modified because the Vehicle Pre-sold is checked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OUV_Live_Status_Change</fullName>
        <active>true</active>
        <errorConditionFormula>( $UserRole.DeveloperName &lt;&gt; &apos;Global_OUV_Office&apos; &amp;&amp;
NOT( ISCHANGED( OUV_ByPassValidation__c )) &amp;&amp;
ISCHANGED( OUV_VehicleMasterStatus__c ) &amp;&amp; ISPICKVAL(PRIORVALUE(OUV_VehicleMasterStatus__c ),&apos;Live&apos;) ) 
&amp;&amp;
( TEXT(OUV_VehicleMasterStatus__c) = &apos;Pre-Live&apos; ||
( TEXT(OUV_VehicleMasterStatus__c) = &apos;Unavailable&apos; &amp;&amp; TEXT(OUV_VehicleSubStatus__c) &lt;&gt; &quot;Write-Off&quot; ) )</errorConditionFormula>
        <errorDisplayField>OUV_VehicleMasterStatus__c</errorDisplayField>
        <errorMessage>Master Status &quot;Live&quot; can be only changed to &quot;Handed In&quot; or &quot;Disposed&quot; with sub status &quot;Write-Off&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OUV_Sub_Status_Required</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(OUV_VehicleSubStatus__c)= &apos;&apos; &amp;&amp;   $Profile.Name  &lt;&gt;  &apos;System Administrator&apos;</errorConditionFormula>
        <errorDisplayField>OUV_VehicleSubStatus__c</errorDisplayField>
        <errorMessage>Sub-Status is a required field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OUV_Vehicle_Sales_Fields</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( ISCHANGED( OUV_ByPassValidation__c ) ),OR 
( 
AND 
( 
ISPICKVAL( OUV_VehicleMasterStatus__c , &quot;Unavailable&quot; ) , 
ISPICKVAL( OUV_VehicleSubStatus__c , &quot;Sold&quot;) , 
ISBLANK(TEXT(OUV_SoldTo__c)) 
), 
AND 
( 
ISPICKVAL( OUV_VehicleMasterStatus__c , &quot;Unavailable&quot; ) , 
ISPICKVAL( OUV_VehicleSubStatus__c , &quot;Sold&quot;) , 
ISNULL( OUV_FinalSalesPrice__c) 
), 
AND 
( 
ISPICKVAL( OUV_VehicleMasterStatus__c , &quot;Unavailable&quot; ) , 
ISPICKVAL( OUV_VehicleSubStatus__c , &quot;Sold&quot;) , 
ISNULL( OUV_SalePriceDate__c) 
) 
)
)</errorConditionFormula>
        <errorMessage>Please fill following fields:                                           
1. Sales Channel                                            
2. Sales Price     
3. Date of Sale</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OUV_BasicUser_MasterStatus</fullName>
        <active>true</active>
        <description>Restrict Basic User from changing OUV Vehicle Master status from Handed in to Live.</description>
        <errorConditionFormula>AND(
$UserRole.DeveloperName &lt;&gt; &apos;Global_OUV_Office&apos;,
$Profile.Name =&apos;OUV Basic User Platform&apos;,
ISCHANGED(OUV_VehicleMasterStatus__c),
ISPICKVAL(PRIORVALUE(OUV_VehicleMasterStatus__c),&apos;Handed In&apos;),
ISPICKVAL(OUV_VehicleMasterStatus__c , &apos;Live&apos;) 
)</errorConditionFormula>
        <errorMessage>Insufficient permissions to change Master Status from &apos;Handed in&apos; to &apos;Live&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OUV_Check_Primary_Location</fullName>
