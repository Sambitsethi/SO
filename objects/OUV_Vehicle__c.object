    <fields>
        <fullName>OUV_ExpectedHandInDate__c</fullName>
        <externalId>false</externalId>
        <formula>IF(

      AND(

         ISPICKVAL(OUV_VehicleMasterStatus__c, &apos;Pre-Live&apos;), 
         OUV_IsReplacementVehicle__c = false 


       ) ,

         OUV_ProposedHandInDate__c, 


       IF(
 
           AND( 

               ISPICKVAL(OUV_VehicleMasterStatus__c, &apos;Live&apos;) ,
               NOT( ISPICKVAL(OUV_NewRequestedHandedInDateStatus__c, &apos;Approved&apos;) ) ,
               OUV_IsReplacementVehicle__c = false,
               ISBLANK(OUV_ApprovedNewRequestedHandInDate__c) 

           ) , 

               OUV_ProposedHandInDate__c - OUV_ProposedAddToFleetDate__c + OUV_AddToFleetDate__c, 



              IF(

                  ISPICKVAL(OUV_VehicleMasterStatus__c, &apos;Handed In&apos;) ,  
                  OUV_ActualHandInDate__c, 

                  IF(

                      AND(
                          ISPICKVAL(OUV_NewRequestedHandedInDateStatus__c, &apos;Approved&apos;) ,
                          NOT( ISBLANK(OUV_ApprovedNewRequestedHandInDate__c) ) ,

                          OUV_IsReplacementVehicle__c = false 

                   ) ,

                         OUV_ApprovedNewRequestedHandInDate__c, 


                       IF(

                           AND(
    
                               ISPICKVAL(OUV_NewRequestedHandedInDateStatus__c, &apos;Approved&apos;) ,
                               NOT( ISBLANK(OUV_ApprovedNewRequestedHandInDate__c) ) 

                          ) , 

                               OUV_ApprovedNewRequestedHandInDate__c, 


                               IF(

                                  OUV_IsReplacementVehicle__c = true, 

                                  OUV_HandinDate__c,
                                  OUV_HandinDate__c) 

                                  ) 

                             ) 

                       ) 



                  ) 

             )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Forecast/Actual Hand In Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
