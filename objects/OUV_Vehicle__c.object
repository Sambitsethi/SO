        <errorConditionFormula>IF(AND(NOT( ISCHANGED( OUV_ByPassValidation__c ) ),( NOT(ISBLANK(OUV_NewRequestedHandedInDate__c)) &amp;&amp; (OUV_NewRequestedHandedInDate__c &lt; OUV_HandinDate__c ))),true,false)</errorConditionFormula>
        <errorDisplayField>OUV_NewRequestedHandedInDate__c</errorDisplayField>
        <errorMessage>New Requested Handed In Date should be greater than Expected Handed In</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OUV_Vehicle_Required_Justification_Field</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( ISCHANGED( OUV_ByPassValidation__c ) ),
OR 
	( 
		AND 
		( 
			ISPICKVAL( OUV_VehicleMasterStatus__c , &quot;Unavailable&quot; ) , 
			ISPICKVAL( OUV_VehicleSubStatus__c , &quot;Write-Off&quot;),
			ISCHANGED(OUV_VehicleMasterStatus__c),
			ISCHANGED(OUV_VehicleSubStatus__c),
			ISBLANK(OUV_Justification__c)

		),
		AND
		(
			ISPICKVAL( OUV_UpdatedDisposalRoute__c , &quot;Scrap&quot; ) ,
			ISCHANGED(OUV_UpdatedDisposalRoute__c),
			ISBLANK(OUV_Justification__c)
		), 
		AND
		( 
			ISPICKVAL( OUV_NewRequestedHandedInDateStatus__c , &quot;New&quot; ) , 
			NOT(ISBLANK(OUV_NewRequestedHandedInDate__c)),
			ISBLANK(OUV_Justification__c)
		)
	))</errorConditionFormula>
        <errorDisplayField>OUV_Justification__c</errorDisplayField>
        <errorMessage>Please add a justification to complete this update.</errorMessage>
    </validationRules>
