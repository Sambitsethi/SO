    <validationRules>
        <fullName>OUV_Vehicle_Transfer_Manager</fullName>
        <active>true</active>
        <description>When user changes the owner of OUV vehicle check if  No of Keys Transferred to New Manager pick list and Keys Transferred to New Manager checkbox should be populated</description>
        <errorConditionFormula>AND 
( NOT( ISCHANGED( OUV_ByPassValidation__c ) ),
OR( 
ISCHANGED( OwnerId ) , 
AND( 
ISCHANGED(OUV_VehicleMasterStatus__c), 
ISPICKVAL(OUV_VehicleMasterStatus__c, &quot;Handed In&quot;)) 
), 
OUV_KeysTransfferedToNewManager__c = false , 
OR 
( 
NOT(ISPICKVAL(OUV_NoOfKeysTransferredToNewManager__c, &quot;0&quot; )), 
NOT(ISPICKVAL(OUV_NoOfKeysTransferredToNewManager__c, &quot;1&quot;)), 
NOT(ISPICKVAL(OUV_NoOfKeysTransferredToNewManager__c, &quot;2&quot;)) 
) 
)</errorConditionFormula>
