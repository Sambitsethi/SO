    <fields>
        <fullName>OUV_Age__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISPICKVAL(OUV_VehicleMasterStatus__c, &quot;Handed In&quot;) || (ISPICKVAL(OUV_VehicleMasterStatus__c, &quot;Unavailable&quot;) &amp;&amp; ISPICKVAL( OUV_VehicleSubStatus__c , &quot;Sold&quot;))  , 
IF(
   ISNULL(OUV_NewRequestedHandedInDate__c), 
   MONTH(TODAY()) - MONTH(OUV_HandinDate__c),
   IF(
      ISPICKVAL(OUV_NewRequestedHandedInDateStatus__c ,&apos;Approved&apos;),
      MONTH(TODAY()) - MONTH(OUV_NewRequestedHandedInDate__c),
      NULL
     )
  ),
NULL )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
