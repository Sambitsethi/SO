    <validationRules>
        <fullName>Format_Year_End</fullName>
        <active>true</active>
        <description>Check that the string is a number so it displays as a year. Enforce 4 charcters and ensure it is &gt; 1890</description>
        <errorConditionFormula>NOT(ISBLANK(SO_Model_Year_End__c )) &amp;&amp;(
NOT(ISNUMBER(  SO_Model_Year_End__c )) || LEN( SO_Model_Year_End__c ) != 4 || VALUE( SO_Model_Year_End__c ) &lt; 1890)</errorConditionFormula>
        <errorDisplayField>SO_Model_Year_End__c</errorDisplayField>
        <errorMessage>The year must be a valid 4 digit year greater than 1889.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_before_Start</fullName>
        <active>true</active>
        <description>Check that the end is not before the start year</description>
        <errorConditionFormula>NOT(ISBLANK( SO_Model_Year_End__c )) &amp;&amp; NOT(ISBLANK( SO_Model_Year_Start__c )) &amp;&amp; VALUE(SO_Model_Year_End__c ) &lt; VALUE(SO_Model_Year_Start__c )</errorConditionFormula>
        <errorDisplayField>SO_Model_Year_End__c</errorDisplayField>
        <errorMessage>You cannot enter a end year that is before the start year.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Manage_JLR_Model</fullName>
        <active>true</active>
        <errorConditionFormula>( ISNEW() || ISCHANGED(Name) || ISCHANGED(SO_Model_Year_End__c) || ISCHANGED(SO_Model_Year_Start__c) ) &amp;&amp; NOT(  $Permission.SO_Manage_JLR_Models ) &amp;&amp;  SO_Manufacturer__r.SO_JLR_Owned_Brand__c</errorConditionFormula>
        <errorMessage>You are not authorised to make changes to JLR Models</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Format_Year_Start</fullName>
        <active>true</active>
        <description>Check that the string is a number so it displays as a year. Enforce 4 charcters and ensure it is &gt; 1890</description>
        <errorConditionFormula>NOT(ISBLANK(SO_Model_Year_Start__c )) &amp;&amp;(
NOT(ISNUMBER( SO_Model_Year_Start__c )) ||  LEN(SO_Model_Year_Start__c) != 4 ||  VALUE(SO_Model_Year_Start__c) &lt; 1890)</errorConditionFormula>
        <errorDisplayField>SO_Model_Year_Start__c</errorDisplayField>
        <errorMessage>The year must be a valid 4 digit year greater than 1889.</errorMessage>
    </validationRules>
