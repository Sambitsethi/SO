    <fields>
        <fullName>Expiry_Date__c</fullName>
        <externalId>false</externalId>
        <formula>DATE( 
    YEAR( Start_Date__c ) + FLOOR( ( MONTH ( Start_Date__c -1 ) + Duration__c - 1 ) / 12) - IF(MONTH(Start_Date__c)=1,1,0), 

    MOD( MONTH ( Start_Date__c - 1 ) + Duration__c - 1 , 12 )+ 1,  

    IF( DAY( Start_Date__c - 1) &gt; CASE( MOD( MONTH( Start_Date__c - 1) + Duration__c - 1, 12 ) + 1, 
    2, IF(MOD(YEAR( Start_Date__c ) + FLOOR( ( MONTH ( Start_Date__c -1 ) + Duration__c - 1 ) / 12) - IF(MONTH(Start_Date__c)=1,1,0),4)&gt;0,28,29), 
    4, 30, 
    6, 30, 
    9, 30, 
    11, 30, 
    31 ), 
    CASE( MOD( MONTH( Start_Date__c - 1) + Duration__c - 1, 12 ) + 1, 
    2, IF(MOD(YEAR( Start_Date__c ) + FLOOR( ( MONTH ( Start_Date__c -1 ) + Duration__c - 1 ) / 12) - IF(MONTH(Start_Date__c)=1,1,0),4)&gt;0,28,29),
    4, 30, 
    6, 30, 
    9, 30, 
    11, 30, 
    31 ) 
    , DAY( Start_Date__c - 1) 
    ) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Expiry Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
