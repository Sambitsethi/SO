        <fullName>Contract__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Contract Appearance is performed under</description>
        <externalId>false</externalId>
        <label>Contract</label>
        <lookupFilter>
            <active>true</active>
            <errorMessage>Contract does not belong to the selected Advocate.</errorMessage>
            <filterItems>
                <field>$Source.Advocate__c</field>
                <operation>equals</operation>
                <valueField>PRT_Contract__c.Advocate__c</valueField>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>PRT_Contract__c</referenceTo>
        <relationshipLabel>Appearances</relationshipLabel>
        <relationshipName>Talent_Appearances</relationshipName>
        <required>false</required>
