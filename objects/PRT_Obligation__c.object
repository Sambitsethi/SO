    <validationRules>
        <fullName>Expiry_on_or_after_Start</fullName>
        <active>true</active>
        <errorConditionFormula>Expiry_Date__c &lt;  Start_Date__c</errorConditionFormula>
        <errorDisplayField>Expiry_Date__c</errorDisplayField>
        <errorMessage>It cannot expiry before it has started.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Type_Restricted_Require_Hours</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Type__c , &quot;Personal Appearance (Time Restricted)&quot;),   ISNULL(Time_Restriction_Hours__c) )</errorConditionFormula>
        <errorDisplayField>Time_Restriction_Hours__c</errorDisplayField>
        <errorMessage>You must record the number of hours when selecting a type of Personal Appearance (Time Restricted)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Additional_Fee_Required_if_Per_Appearanc</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(  Additional_Fee_Type__c , &quot;Per Appearance&quot;) ,ISNULL(  Additional_Fee__c  ))</errorConditionFormula>
