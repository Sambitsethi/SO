    <fields>
        <fullName>SO_Age_Sent__c</fullName>
        <externalId>false</externalId>
        <formula>IF(  
  ISBLANK( SO_Proposal_Sent_Date__c ),
  NULL,
  IF( 
     OR(
       ISBLANK( SO_Outcome_Date__c ) ,
       NOT(ISPICKVAL( Status , &apos;Outcome&apos;))
     ),
     TODAY() - SO_Proposal_Sent_Date__c,
     DATEVALUE(SO_Outcome_Date__c)  - SO_Proposal_Sent_Date__c
    
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Age Sent</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Number</type>
        <unique>false</unique>
