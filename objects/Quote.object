    <validationRules>
        <fullName>SO_VIP_Move_1_Status_at_a_time</fullName>
        <active>true</active>
        <description>Restrict users to skip status or move backwards</description>
        <errorConditionFormula>((RecordType.DeveloperName = &apos;SO_Private_Office&apos; || RecordType.DeveloperName = &apos;SO_Private_Office_Closed&apos;) &amp;&amp; 
((ISPICKVAL(PRIORVALUE(Status),&quot;Specification&quot;) &amp;&amp; ((ISPICKVAL(Status,&apos;Outcome&apos;) &amp;&amp; ISPICKVAL(SO_Outcome__c,&apos;Accepted&apos;))|| ISPICKVAL(Status,&apos;Proposal Sent&apos;))) || 
(ISPICKVAL(PRIORVALUE(Status),&quot;Price&quot;) &amp;&amp; ((ISPICKVAL(Status,&apos;Outcome&apos;)  &amp;&amp; ISPICKVAL(SO_Outcome__c,&apos;Accepted&apos;)) || ISPICKVAL(Status,&apos;Specification&apos;))) || 
(ISPICKVAL(PRIORVALUE(Status),&quot;Proposal Sent&quot;) &amp;&amp; (ISPICKVAL(Status,&apos;Price&apos;) || ISPICKVAL(Status,&apos;Specification&apos;)))))</errorConditionFormula>
        <errorMessage>Please follow the process by not skipping stages or moving backwards.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_VIP_Mandate_Retailer_Contact</fullName>
        <active>true</active>
        <description>Mandate Retailer contact field during creation only when the Opportunity Sales Type is &apos;Retailer Referral&apos;</description>
        <errorConditionFormula>RecordType.DeveloperName = &apos;SO_Private_Office&apos; &amp;&amp;  ISPICKVAL(Opportunity.Type,&apos;Retailer Referral&apos;) &amp;&amp; ISBLANK( SO_Retailer_Contact__c )</errorConditionFormula>
        <errorDisplayField>SO_Retailer_Contact__c</errorDisplayField>
        <errorMessage>Please enter Retailer Contact</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Bespk_Move_1_Status_at_a_time</fullName>
        <active>true</active>
        <description>Restrict users to skip status or move backwards</description>
        <errorConditionFormula>((RecordType.DeveloperName = &apos;SO_Bespoke&apos; || RecordType.DeveloperName = &apos;SO_Bespoke_Price&apos; || RecordType.DeveloperName = &apos;SO_Bespoke_Closed&apos;) &amp;&amp; 
((ISNEW() &amp;&amp; NOT(ISPICKVAL(Status,&apos;Design Brief Draft&apos;))) ||
(ISPICKVAL(PRIORVALUE(Status),&quot;Design Brief Draft&quot;) &amp;&amp; ((ISPICKVAL(Status,&apos;Render Pack Received&apos;) &amp;&amp;  SO_Design_Brief__r.SO_Requires_Design_Team__c == TRUE) || (ISPICKVAL(Status,&apos;Price&apos;) &amp;&amp; SO_Design_Brief__r.SO_Requires_Design_Team__c == TRUE) || ISPICKVAL(Status,&apos;Proposal Sent&apos;))) || 
(ISPICKVAL(PRIORVALUE(Status),&quot;Design Brief Submitted&quot;) &amp;&amp; (ISPICKVAL(Status,&apos;Design Brief Draft&apos;) || ISPICKVAL(Status,&apos;Price&apos;) || ISPICKVAL(Status,&apos;Proposal Sent&apos;))) || 
(ISPICKVAL(PRIORVALUE(Status),&quot;Render Pack Received&quot;) &amp;&amp; (ISPICKVAL(Status,&apos;Design Brief Draft&apos;) || ISPICKVAL(Status,&apos;Design Brief Submitted&apos;) || ISPICKVAL(Status,&apos;Proposal Sent&apos;))) || 
(ISPICKVAL(PRIORVALUE(Status),&quot;Price&quot;) &amp;&amp; (ISPICKVAL(Status,&apos;Design Brief Draft&apos;) || ISPICKVAL(Status,&apos;Design Brief Submitted&apos;) || ISPICKVAL(Status,&apos;Render Pack Received&apos;))) || 
(ISPICKVAL(PRIORVALUE(Status),&quot;Proposal Sent&quot;) &amp;&amp; (ISPICKVAL(Status,&apos;Design Brief Draft&apos;) || ISPICKVAL(Status,&apos;Design Brief Submitted&apos;) || ISPICKVAL(Status,&apos;Render Pack Received&apos;) || ISPICKVAL(Status,&apos;Price&apos;))) ))</errorConditionFormula>
        <errorMessage>Please follow the process by not skipping stages or moving backwards; Create new version if required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Contact_should_be_from_Retailer</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(SO_Retailer_Contact__c)) &amp;&amp;  SO_Retailer_Contact__r.AccountId &lt;&gt; Opportunity.AccountId</errorConditionFormula>
        <errorDisplayField>SO_Retailer_Contact__c</errorDisplayField>
        <errorMessage>Select a Contact from the Retailer Account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Bespk_Retailer_contact_mandatory</fullName>
        <active>true</active>
        <description>Retailer contact field should be mandatory  for so bespoke pricing and so bespoke layouts if we don&apos;t have any value in related opportunity retailer contact field.</description>
        <errorConditionFormula>AND(
	(RecordType.DeveloperName = &quot;SO_Bespoke&quot; || RecordType.DeveloperName = &quot;SO_Bespoke_Price&quot;),
	(ISBLANK(Opportunity.SO_Retailer_Contact__c)  &amp;&amp; ISBLANK(SO_Retailer_Contact__c) 
	))</errorConditionFormula>
        <errorDisplayField>SO_Retailer_Contact__c</errorDisplayField>
        <errorMessage>Please select Retailer contact to proceed further</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Generic_Cannot_change_Quote_Name</fullName>
        <active>true</active>
        <errorConditionFormula>(RecordType.DeveloperName == &apos;SO_Bespoke&apos; || RecordType.DeveloperName == &apos;SO_Private_Office&apos;) &amp;&amp; NOT(ISNEW()) &amp;&amp; ISCHANGED(Name)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>You cannot change Quote Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_VIP_Mandate_Pricing_before_Price</fullName>
        <active>true</active>
        <errorConditionFormula>(RecordType.DeveloperName = &apos;SO_Private_Office&apos; || RecordType.DeveloperName = &apos;SO_Private_Office_Closed&apos;) &amp;&amp; ISPICKVAL(PRIORVALUE(Status),&quot;Specification&quot;) &amp;&amp; ISPICKVAL(Status,&quot;Price&quot;) &amp;&amp; ISBLANK(SO_Retail_Price__c)</errorConditionFormula>
        <errorMessage>Please enter Retail Price before proceeding further</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Check_for_Preferred_Contact_Email</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
        NOT(ISBLANK( SO_Retailer_Contact__c )) &amp;&amp; ISBLANK( SO_Retailer_Contact__r.Email ),
(
	AND(
	(RecordType.DeveloperName = &quot;SO_Bespoke&quot; || RecordType.DeveloperName = &quot;SO_Bespoke_Price&quot;|| RecordType.DeveloperName = &quot;SO_Bespoke_Closed&quot;),
  	(ISBLANK(Opportunity.SO_Retailer_Contact__r.Email)  &amp;&amp; ISBLANK(SO_Retailer_Contact__c) )
	)

),
(
    And(
        (RecordType.DeveloperName = &quot;SO_Private_Office&quot;  || RecordType.DeveloperName = &quot;SO_Private_Office_Closed&quot;),
        ( NOT(ISBLANK( Opportunity.SO_Retailer_Contact__c)) &amp;&amp; ISBLANK(Opportunity.SO_Retailer_Contact__r.Email)  &amp;&amp; ISBLANK(SO_Retailer_Contact__c) )
       )
) )</errorConditionFormula>
        <errorDisplayField>SO_Retailer_Contact__c</errorDisplayField>
        <errorMessage>Retailer Contact or Retailer contact linked with opportunity should have Email ID</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Bespk_Mandate_Pricing_before_Price</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.DeveloperName = &apos;SO_Bespoke&apos; &amp;&amp; ISPICKVAL(PRIORVALUE(Status),&quot;Render Pack Received&quot;) &amp;&amp; ISPICKVAL(Status,&quot;Price&quot;) &amp;&amp; 
(ISBLANK(SO_Retail_Price__c) || ISBLANK(SO_Revenue__c) || ISBLANK(SO_Contribution__c) || ISBLANK(SO_Cost_Price__c) || ISBLANK(SO_List_Price__c) )</errorConditionFormula>
        <errorMessage>Please complete Pricing information before proceeding further</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Bespk_Validate_on_Submitted_status</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.DeveloperName = &apos;SO_Bespoke&apos; &amp;&amp;(ISPICKVAL(Status,&apos;Design Brief Submitted&apos;) &amp;&amp; ISCHANGED(Status) &amp;&amp; NOT(ISPICKVAL( SO_Design_Brief__r.SO_Status__c ,&apos;Submitted&apos;)) &amp;&amp; RecordType.DeveloperName == &apos;SO_Bespoke&apos;)</errorConditionFormula>
        <errorMessage>Please submit related Design Brief before submitting Quote</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Generic_Outcome_Initiated_by_Other</fullName>
        <active>true</active>
        <errorConditionFormula>(RecordType.DeveloperName = &apos;SO_Bespoke&apos; || RecordType.DeveloperName = &apos;SO_Bespoke_Price&apos; || RecordType.DeveloperName = &apos;SO_Bespoke_Closed&apos; || RecordType.DeveloperName = &apos;SO_Private_Office&apos; || RecordType.DeveloperName = &apos;SO_Private_Office_Closed&apos;) &amp;&amp; ISPICKVAL(SO_Outcome_Initiated_By__c,&apos;Other&apos;) &amp;&amp; ISBLANK(SO_Outcome_Initiated_By_Other__c)</errorConditionFormula>
        <errorDisplayField>SO_Outcome_Initiated_By_Other__c</errorDisplayField>
        <errorMessage>Please enter Outcome Initiated By Other</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Generic_Cannot_reopen_Outcome_Quote</fullName>
        <active>true</active>
        <errorConditionFormula>((RecordType.DeveloperName = &apos;SO_Bespoke&apos; || RecordType.DeveloperName = &apos;SO_Bespoke_Closed&apos; || RecordType.DeveloperName = &apos;SO_Bespoke_Price&apos; || RecordType.DeveloperName = &apos;SO_Private_Office&apos; || RecordType.DeveloperName = &apos;SO_Private_Office_Closed&apos;) &amp;&amp; 
ISPICKVAL(PRIORVALUE(Status),&quot;Outcome&quot;) &amp;&amp; NOT(ISPICKVAL(Status,&apos;Outcome&apos;)) )</errorConditionFormula>
        <errorMessage>Cannot reopen a Quote in Outcome stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Bespk_Cannot_Amend_without_submitting</fullName>
        <active>true</active>
        <description>A Quote cannot be rejected by saying &apos;Design Brief Amend&apos; or &apos;Render Pack Amend&apos; without submitting the Design Brief</description>
        <errorConditionFormula>RecordType.DeveloperName = &apos;SO_Bespoke&apos; &amp;&amp; (ISCHANGED( SO_Outcome_Detail__c ) &amp;&amp; (ISPICKVAL(SO_Outcome_Detail__c ,&apos;Render Pack Amend&apos;) || ISPICKVAL(SO_Outcome_Detail__c ,&apos;Design Brief Amend&apos;)) &amp;&amp; ISPICKVAL( SO_Design_Brief__r.SO_Status__c ,&apos;Draft&apos;))</errorConditionFormula>
        <errorMessage>Please modify the existing Design Brief and submit it instead of amending</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Bespk_Validate_Render_Received_status</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.DeveloperName = &apos;SO_Bespoke&apos; &amp;&amp;(ISPICKVAL(Status,&apos;Render Pack Received&apos;) &amp;&amp; ISCHANGED(Status) &amp;&amp; NOT(ISPICKVAL( SO_Design_Brief__r.SO_Status__c ,&apos;Returned&apos;)) &amp;&amp; RecordType.DeveloperName == &apos;SO_Bespoke&apos;)</errorConditionFormula>
        <errorMessage>Please note the design brief should be in &apos;Returned&apos; status to move to this stage.</errorMessage>
    </validationRules>
