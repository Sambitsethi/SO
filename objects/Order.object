    <fields>
        <fullName>SO_Is_Closed__c</fullName>
        <description>Determines if the order is closed successfully and type of SO Classic</description>
        <externalId>false</externalId>
        <formula>IF( 
  AND(
    AND(
      ISPICKVAL(SO_Status__c , &apos;Outcome&apos;),
      ISPICKVAL(SO_Outcome__c , &apos;Retailed&apos;)
    ),
    OR(
       RecordType.DeveloperName = &apos;SO_Donor_Build&apos;,
       RecordType.DeveloperName = &apos;SO_Classic_Build_Order&apos;,
       RecordType.DeveloperName = &apos;Classic_Service_Order&apos;,
       RecordType.DeveloperName = &apos;SO_Classic_Works_Legend_Order&apos;
    )
  ),
  true,
  false
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Closed Retailed</label>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
