    <validationRules>
        <fullName>SO_CLS_Gen_Blank_Outcome</fullName>
        <active>false</active>
        <errorConditionFormula>(BEGINS(RecordType.DeveloperName,&apos;SO_&apos;) || RecordType.DeveloperName == &apos;Classic_Service_Order&apos;) &amp;&amp; RecordType.DeveloperName &lt;&gt; &apos;SO_Classic_Parts_Order&apos; &amp;&amp;
ISCHANGED(SO_Status__c) &amp;&amp; ISPICKVAL(SO_Status__c, &apos;Outcome&apos;) &amp;&amp; ISBLANK(TEXT(SO_Outcome__c))</errorConditionFormula>
        <errorMessage>Please enter Outcome to close the Order</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Cannot_Change_From_Outcome</fullName>
        <active>false</active>
        <errorConditionFormula>(BEGINS(RecordType.DeveloperName,&apos;SO_&apos;) || RecordType.DeveloperName == &apos;Classic_Service_Order&apos;) &amp;&amp; RecordType.DeveloperName &lt;&gt; &apos;SO_Classic_Parts_Order&apos; &amp;&amp; ISCHANGED( SO_Status__c ) &amp;&amp;  ISPICKVAL( PRIORVALUE(SO_Status__c ) , &apos;Outcome&apos;)</errorConditionFormula>
        <errorMessage>You cannot change the stage once in outcome</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_CLS_Future_Handover_Outcome</fullName>
        <active>false</active>
        <errorConditionFormula>(RecordType.DeveloperName == &apos;SO_Classic_Bespoke_Build_Order&apos; || RecordType.DeveloperName == &apos;SO_Donor_Build&apos; || RecordType.DeveloperName == &apos;SO_Classic_Build_Order&apos; || RecordType.DeveloperName == &apos;Classic_Service_Order&apos; || RecordType.DeveloperName == &apos;SO_Classic_Works_Legend_Order&apos;) &amp;&amp; ISCHANGED(SO_Status__c) &amp;&amp; ISPICKVAL(SO_Status__c, &apos;Outcome&apos;) &amp;&amp; NOT(ISBLANK(TEXT(SO_Outcome__c))) &amp;&amp; 
ISPICKVAL(SO_Outcome__c, &apos;Retailed&apos;) &amp;&amp; NOT(ISBLANK(SO_Customer_Handover_Date__c )) &amp;&amp; SO_Customer_Handover_Date__c &lt; TODAY()</errorConditionFormula>
        <errorDisplayField>SO_Customer_Handover_Date__c</errorDisplayField>
        <errorMessage>Customer Handover Date cannot be in the past</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_CLS_Mandatory_from_In_Build</fullName>
        <active>false</active>
        <errorConditionFormula>(RecordType.DeveloperName = &apos;SO_Donor_Build&apos; || RecordType.DeveloperName = &apos;SO_Classic_Build_Order&apos;  ||
RecordType.DeveloperName = &apos;SO_Classic_Bespoke_Build_Order&apos; ) &amp;&amp; ISCHANGED(SO_Status__c) &amp;&amp; 
NOT(RIGHT(SO_Pinewood_Job_Card__c , 8) = &apos;Migrated&apos; )&amp;&amp;
(SO_Build_Started__c == FALSE) &amp;&amp; (ISPICKVAL(SO_Status__c, &apos;In Build&apos;) || ISPICKVAL(SO_Status__c, &apos;Accepted by Sales&apos;) || (ISPICKVAL(SO_Status__c, &apos;Outcome&apos;) &amp;&amp; ISPICKVAL(SO_Outcome__c, &apos;Retailed&apos;)))</errorConditionFormula>
        <errorMessage>You can move forward only after the Build has started.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_CLS_Mandatory_FMS_at_Outcome</fullName>
        <active>false</active>
        <errorConditionFormula>(RecordType.DeveloperName == &apos;SO_Donor_Build&apos; || RecordType.DeveloperName == &apos;SO_Classic_Build_Order&apos; || RecordType.DeveloperName == &apos;SO_Classic_Works_Legend_Order&apos; ) &amp;&amp; 
ISCHANGED(SO_Status__c) &amp;&amp; ISPICKVAL(SO_Status__c, &apos;Outcome&apos;) &amp;&amp; NOT(ISBLANK(TEXT(SO_Outcome__c))) &amp;&amp; ISPICKVAL(SO_Outcome__c, &apos;Retailed&apos;) &amp;&amp; 
(SO_FMS_Disposal__c == FALSE || SO_FMS_Passout__c == FALSE )</errorConditionFormula>
        <errorMessage>Please complete FMS Passout obtained &amp; FMS Disposal under Handover details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Cannot_Change_Outcome_Reason</fullName>
        <active>false</active>
        <errorConditionFormula>(BEGINS(RecordType.DeveloperName,&apos;SO_&apos;) || RecordType.DeveloperName == &apos;Classic_Service_Order&apos;) &amp;&amp; RecordType.DeveloperName &lt;&gt; &apos;SO_Classic_Parts_Order&apos; &amp;&amp; ISCHANGED(SO_Outcome__c  ) &amp;&amp;  ISPICKVAL(SO_Status__c, &apos;Outcome&apos;) &amp;&amp;  NOT(ISBLANK( PRIORVALUE(SO_Outcome__c  ) ) )</errorConditionFormula>
        <errorMessage>You cannot change the outcome reason once set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_CLS_Cannot_Move_Back_From_ABS</fullName>
        <active>false</active>
        <errorConditionFormula>(RecordType.DeveloperName = &apos;SO_Donor_Build&apos; || RecordType.DeveloperName = &apos;SO_Classic_Build_Order&apos; || 
RecordType.DeveloperName = &apos;SO_Classic_Bespoke_Build_Order&apos;) &amp;&amp; ISCHANGED( SO_Status__c ) &amp;&amp; ISPICKVAL( PRIORVALUE(SO_Status__c ) , &apos;Accepted by Sales&apos;) &amp;&amp; NOT(ISPICKVAL( SO_Status__c  , &apos;Outcome&apos;)) &amp;&amp;  SO_ABS_Complete__c = true</errorConditionFormula>
        <errorMessage>This order has already been ABS Complete and cannot be moved backwards.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Check_Matches_Opp_Record_type</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
 NOT(ISBLANK( OpportunityId )),
 OR(
   AND( Opportunity.RecordType.DeveloperName = &apos;SO_Continuation&apos;, NOT( RecordType.DeveloperName = &apos;SO_Classic_Build_Order&apos;) ),
   AND( Opportunity.RecordType.DeveloperName = &apos;SO_Limited_Edition&apos;, NOT( RecordType.DeveloperName = &apos;SO_Donor_Build&apos;) ),
   AND( Opportunity.RecordType.DeveloperName = &apos;SO_Reborn&apos;, NOT( RecordType.DeveloperName = &apos;SO_Donor_Build&apos;) ),
   AND( Opportunity.RecordType.DeveloperName = &apos;SO_Service&apos;, NOT( RecordType.DeveloperName = &apos;Classic_Service_Order&apos;) ),
   AND( Opportunity.RecordType.DeveloperName = &apos;SO_Classic_Bespoke&apos;, NOT( RecordType.DeveloperName = &apos;SO_Classic_Bespoke_Build_Order&apos;) ),
   AND( Opportunity.RecordType.DeveloperName = &apos;SO_Works_Legends&apos;, NOT( RecordType.DeveloperName = &apos;SO_Classic_Works_Legend_Order&apos;) )
 )
)</errorConditionFormula>
        <errorMessage>This type of order is not applicable to the associated opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_CLS_Mandatory_Vehicle_at_Outcome</fullName>
