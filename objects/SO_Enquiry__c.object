    <validationRules>
        <fullName>SO_New_to_Contacted</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISCHANGED(SO_Bypass_Validation_Rules__c )) &amp;&amp; (
ISBLANK(SO_SLA_First_Contact_Complete__c)) &amp;&amp;
(ISPICKVAL(SO_Stage__c, &apos;Contacted&apos;) ||
 ISPICKVAL(SO_Stage__c, &apos;Discovery&apos;) ||
(ISPICKVAL(SO_Stage__c, &apos;Closed&apos;) &amp;&amp; (
ISPICKVAL( SO_Closed_State__c , &apos;Qualified&apos;) ||
ISPICKVAL( SO_Closed_State__c , &apos;Qualified - KMI&apos;)
)
)
) &amp;&amp; NOT(ISNEW())</errorConditionFormula>
        <errorMessage>You must have recorded your contact activity. Please create a log of your activity to continue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Mandate_Closed_Reason</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISCHANGED(SO_Bypass_Validation_Rules__c )) &amp;&amp; ISPICKVAL(SO_Closed_State__c,&apos;Not Qualified&apos;) &amp;&amp; ISBLANK(TEXT(SO_Closed_Reason__c))</errorConditionFormula>
        <errorDisplayField>SO_Closed_Reason__c</errorDisplayField>
        <errorMessage>Please enter Closed Reason for Not Qualified Enquiry</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Contacted_to_Discovery</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISCHANGED(SO_Bypass_Validation_Rules__c )) &amp;&amp; (
ISBLANK(SO_Contact__c)) &amp;&amp;
 (ISPICKVAL(SO_Stage__c, &apos;Discovery&apos;) ||
  (ISPICKVAL(SO_Stage__c, &apos;Closed&apos;) &amp;&amp; (
    ISPICKVAL( SO_Closed_State__c , &apos;Qualified&apos;) ||
    ISPICKVAL( SO_Closed_State__c , &apos;Qualified - KMI&apos;)
   )
  )
) &amp;&amp; NOT(ISNEW())</errorConditionFormula>
        <errorMessage>Please select the contact in the Contact Details section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Change_if_Qualified</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISCHANGED(SO_Bypass_Validation_Rules__c )) &amp;&amp; ISPICKVAL(PRIORVALUE(SO_Closed_State__c),&apos;Qualified&apos;) &amp;&amp;
(
ISCHANGED(SO_Closed_State__c ) ||
ISCHANGED(SO_Closed_Reason__c) ||
ISCHANGED(SO_Retailer__c) ||
ISCHANGED(SO_Region__c) ||
ISCHANGED(SO_Brand__c) ||
ISCHANGED(SO_Model__c) ||
ISCHANGED(SO_Sales_Area__c) ||
ISCHANGED(SO_Market__c) ||
ISCHANGED(SO_Originating_Division__c)
)</errorConditionFormula>
        <errorMessage>You cannot change the enquiry once qualified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Production_year_validation</fullName>
        <active>true</active>
        <description>Check if year is within range of Model&apos;s year range; skips Continuation Programme Type</description>
        <errorConditionFormula>NOT(ISCHANGED(SO_Bypass_Validation_Rules__c )) &amp;&amp; NOT(ISBLANK(SO_Production_Year__c )) &amp;&amp; NOT(ISPICKVAL(SO_Product_Offering__c,&apos;Continuation&apos;)) &amp;&amp; 
(NOT(LEN( SO_Production_Year__c ) == 4) || ISNUMBER(SO_Production_Year__c ) == FALSE || 
IF(NOT(ISBLANK(SO_Series_Name__c)),((NOT(ISBLANK(SO_Series_Name__r.SO_Model_Year_Start__c )) &amp;&amp; VALUE(SO_Production_Year__c ) &lt; VALUE(SO_Series_Name__r.SO_Model_Year_Start__c)) ||
(NOT(ISBLANK(SO_Series_Name__r.SO_Model_Year_End__c )) &amp;&amp; VALUE(SO_Production_Year__c ) &gt; VALUE(SO_Series_Name__r.SO_Model_Year_End__c ))),
IF(NOT(ISBLANK(SO_Model__c)),((NOT(ISBLANK(SO_Model__r.SO_Model_Year_Start__c )) &amp;&amp; VALUE(SO_Production_Year__c ) &lt; VALUE(SO_Model__r.SO_Model_Year_Start__c)) ||
(NOT(ISBLANK(SO_Model__r.SO_Model_Year_End__c )) &amp;&amp; VALUE(SO_Production_Year__c ) &gt; VALUE(SO_Model__r.SO_Model_Year_End__c ))),FALSE)))</errorConditionFormula>
        <errorMessage>Please enter a valid year within corresponding model&apos;s range.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_CLA_Req_Fields_Qualified_KMI</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISCHANGED(SO_Bypass_Validation_Rules__c )) &amp;&amp; (RecordType.DeveloperName == &apos;SO_Classic_Sales&apos; || RecordType.DeveloperName == &apos;SO_Classic_Service&apos; ) &amp;&amp; ISPICKVAL(SO_Stage__c,&apos;Closed&apos;) &amp;&amp; ISPICKVAL(SO_Closed_State__c,&apos;Qualified - KMI&apos;) &amp;&amp; 
(ISBLANK(TEXT(SO_Product_Offering__c)) || 
ISBLANK(SO_Brand__c) ||
ISBLANK(SO_Contact__c) || 
ISBLANK(SO_Model__c))</errorConditionFormula>
        <errorMessage>Please complete the required fields Product Offering, Preferred Contact, Brand &amp; Model before converting to KMI.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_Prevent_Return_to_New_Status</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISCHANGED(SO_Bypass_Validation_Rules__c )) &amp;&amp; ISPICKVAL(SO_Stage__c, &apos;New&apos;) &amp;&amp; ISCHANGED(SO_Stage__c)</errorConditionFormula>
        <errorMessage>You cannot return to the New stage after making contact.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_on_Create_Enquiry_in_New_Stage</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(SO_Bypass_Validation_Rules__c ) &amp;&amp; ISNEW() &amp;&amp; NOT( ISPICKVAL( SO_Stage__c , &apos;New&apos;) )</errorConditionFormula>
