    <validationRules>
        <fullName>Cost_Of_Use_Values_Required</fullName>
        <active>true</active>
        <description>Statutory VAT, Cost Per and Excess Value are required when Hire/Lease is populated</description>
        <errorConditionFormula>NOT(ISBLANK(Hire_Lease__c)) &amp;&amp;  (ISBLANK(Statutory_VAT__c) ||  ISBLANK( TEXT(Cost_Per__c) ) ||  ISBLANK( Excess_Value__c ) ||  ISBLANK( Duration__c ) )</errorConditionFormula>
        <errorMessage>Statutory VAT, Cost Per, Excess Value and Duration are mandatory when Hire/Lease is populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OUV_NOT_FrequentD_FirstLastName_Email</fullName>
        <active>true</active>
        <description>Makes the First Name, Last Name and Email field required if a Frequent Driver was not picked.</description>
        <errorConditionFormula>AND(
ISBLANK( OUV_Frequent_Drivers__c ), 
OR(
ISBLANK( OUV_First_Name__c ),
ISBLANK( OUV_Last_Name__c ),
ISBLANK( OUV_Email__c )),
OR(
ISBLANK( OUV_Encrypted_First_Name__c),
ISBLANK( OUV_Encrypted_Last_Name__c),
ISBLANK( OUV_Encrypted_Email__c)))</errorConditionFormula>
        <errorMessage>Please fill the First Name, Last Name and Email fields</errorMessage>
    </validationRules>
